    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >

        <LinearLayout
            android:id="@+id/linearLayout11"
            android:layout_width="100dp"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:orientation="horizontal" >

            <Button
                android:id="@+id/lilly_1_backBTN"
                android:layout_width="100dip"
                android:layout_height="60dip"
                android:background="@drawable/button_previous"
                android:text=" " />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/LinearLayout01"
            android:layout_width="100dp"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:gravity="center" >

            <Button
                android:id="@+id/lilly_1_text_ToggleButton"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:background="@drawable/button_text_on"
                android:text="" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayout3"
            android:layout_width="100dp"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:gravity="center" >

            <Button
                android:id="@+id/lilly_1_autoRead_ToggleButton"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:background="@drawable/button_speech_off"
                android:text="" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayout12"
            android:layout_width="100dp"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:gravity="right"
            android:orientation="vertical" >

            <Button
                android:id="@+id/lilly_1_fwdBTN"
                android:layout_width="100dip"
                android:layout_height="60dip"
                android:background="@drawable/button_next"
                android:text=" " />
        </LinearLayout>
    </LinearLayout>

</LinearLayout>